
--*******************************************************************
--TOPVISION-QAM-CHANNEL-X-MIB: QAM Channel Extention MIB file
--
--added at 2014.12.3, lvhaiting
--CCMTS-17526 修改qam name的范围  修改OldPid 和 newPid范围支持全PID范围，不再限制不允许配置0   
--CCMTS-17590 修改topCcmtsEqamStreamInfoStatus 的类型定义
--Topvision      
--
--*******************************************************************   

TOPVISION-QAM-CHANNEL-X-MIB DEFINITIONS ::= BEGIN

	IMPORTS    
        MODULE-IDENTITY,
    	OBJECT-TYPE,
    	Integer32, 
    	Unsigned32
        	FROM SNMPv2-SMI 
        qamChannelEntry
            FROM SCTE-HMS-QAM-MIB
		RowStatus, 
		MacAddress,
		DisplayString,
        TruthValue
            FROM SNMPv2-TC
		ifIndex
			FROM IF-MIB
		InetAddress,
		InetAddressType
			FROM INET-ADDRESS-MIB
		QAMChannelModulationFormat, 
		QAMChannelInterleaveMode
			FROM SCTE-HMS-HEADENDIDENT-TC-MIB			
        topCcmtsMibObject
        	FROM  TOPVISION-CCMTS-MIB;                  

topCcmtsQamObject  OBJECT IDENTIFIER ::= { topCcmtsMibObject 24}

-- DescrIPtion
-- Extension to the qam-channel table 

qamChannelXTable	OBJECT-TYPE
	SYNTAX			SEQUENCE OF QamChannelXEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION
		"This table describes the configuration and attributes of each
		QAM channel of the QAM designated by ifIndex.This table contains
        additional objects for the QAM channel table."
    ::= {topCcmtsQamObject 1}
    
qamChannelXEntry	OBJECT-TYPE
	SYNTAX			QamChannelXEntry
	MAX-ACCESS		not-accessible
	STATUS			current
	DESCRIPTION   
		"There is an entry containing additional infomation about QAM channel." 
	AUGMENTS		{ qamChannelEntry }
	::= { qamChannelXTable 1 }
	
QamChannelXEntry ::= SEQUENCE {   
		qamChannelId			Unsigned32,
		qamChannelSymbolRate	Unsigned32		
}   
	
qamChannelId OBJECT-TYPE  
	SYNTAX		Unsigned32
	MAX-ACCESS  read-only
	STATUS		current
	DESCRIPTION
		"The channel ID of EQAM channel"  
	::= { qamChannelXEntry 1}
	
qamChannelSymbolRate OBJECT-TYPE
	SYNTAX		Unsigned32
	MAX-ACCESS	read-only
	STATUS		current
	DESCRIPTION
		"The symbol rate of EQAM channel.
		 Unit: sym/s"
	::= { qamChannelXEntry 2}    

topCcmtsEqamTemplateGlobalTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TopCcmtsEqamTemplateGlobalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "This table describes EQAM template system configuration,
		including EQAM template creation and destroy action."
    ::={ topCcmtsQamObject 2 }

topCcmtsEqamTemplateGlobalEntry OBJECT-TYPE
    SYNTAX        TopCcmtsEqamTemplateGlobalEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry of topCcmtsEqamTemplateGlobalTable with EQAM template ID as index."
    INDEX  { topCcmtsEqamTemplateId }
    ::= { topCcmtsEqamTemplateGlobalTable 1 }

TopCcmtsEqamTemplateGlobalEntry ::= SEQUENCE {
             topCcmtsEqamTemplateId            INTEGER,
             topCcmtsEqamTemplateGlobalJitterTolerance            INTEGER,
             topCcmtsEqamTemplateGlobalIgmpVersion            INTEGER,
             topCcmtsEqamTemplateGlobalVlanCheck            INTEGER,
             topCcmtsEqamTemplateGlobalRowStatus            RowStatus,
             topCcmtsEqamTemplateGlobalDevType     BITS
           }

topCcmtsEqamTemplateId OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "EQAM template ID."
    ::={ topCcmtsEqamTemplateGlobalEntry 1 }

topCcmtsEqamTemplateGlobalJitterTolerance OBJECT-TYPE
    SYNTAX        INTEGER(1..1000) 
    UNITS         "millisecond"
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS streams network jitter time configuration in EQAM template."
    DEFVAL {500}
       ::= { topCcmtsEqamTemplateGlobalEntry 2 }

topCcmtsEqamTemplateGlobalIgmpVersion OBJECT-TYPE
    SYNTAX      INTEGER {
					igmpv2(2),    
					igmpv3(3)
				}
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "The value of this object is the version of the IGMP. 
		The value '2' represents IGMP v2, The value '3' represents 
		IGMP v3."

	DEFVAL {igmpv2}
    ::={ topCcmtsEqamTemplateGlobalEntry 3 }

topCcmtsEqamTemplateGlobalVlanCheck OBJECT-TYPE
    SYNTAX      INTEGER{
					enable(1),    
					disable(2)
				}
    MAX-ACCESS    read-create
    STATUS        current 
    DESCRIPTION
        "Indicates whether EQAM check TS data VLAN
		mismatching TS data will be discarded ."
	DEFVAL {disable}
    ::={ topCcmtsEqamTemplateGlobalEntry 4 }

topCcmtsEqamTemplateGlobalRowStatus OBJECT-TYPE
    SYNTAX        RowStatus
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "table row status."
    ::={ topCcmtsEqamTemplateGlobalEntry 5 }
    
topCcmtsEqamTemplateGlobalDevType OBJECT-TYPE
    SYNTAX  BITS { 
               cc8800a(1),
               cc8800b(2),
               cc8800c_a(3),
               cc8800d(4),
               cc8800c_b(5),
               cc8800s(6),
               cc8800c_10g(7),
               cc8800e(8),    
               cc8800c_e(9),
               cc8800d_e(10),
               cc8800d_p2(11),
	           cc8800d_u2(12),
	           cc8800f_p1(13),
               cc8800f_p2(14),
	           cc8800f_u1(15),
	           cc8800f_u2(16),
	           cc8800e_p2(17),
	           cc8800c_p2(18)
             }
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "RMD device type bits list."
    ::={ topCcmtsEqamTemplateGlobalEntry 6 }

topCcmtsEqamTemplateChnlTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TopCcmtsEqamTemplateChnlEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The EQAM template channel configuration table."
    ::={ topCcmtsQamObject 3 }

topCcmtsEqamTemplateChnlEntry OBJECT-TYPE
    SYNTAX        TopCcmtsEqamTemplateChnlEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry of topCcmtsEqamTemplateChnlTable table,
		with index EQAM template ID 'topCcmtsEqamTemplateId' and ,
		EQAM downstream channel ID 'topCcmtsEqamTemplateChnlChnlId'."
    INDEX  { topCcmtsEqamTemplateId,
           topCcmtsEqamTemplateChnlChnlId           }
    ::= { topCcmtsEqamTemplateChnlTable 1 }

TopCcmtsEqamTemplateChnlEntry ::= SEQUENCE {
             topCcmtsEqamTemplateChnlChnlId            INTEGER,
             topCcmtsEqamTemplateChnlTsIDOffset            INTEGER, 
             topCcmtsEqamTemplateChnlQamGroupName          DisplayString,
             topCcmtsEqamTemplateChnlQamManager            INTEGER,
             topCcmtsEqamTemplateChnlOriginalNetworkId            INTEGER,
             topCcmtsEqamTemplateChnlPatInterval            INTEGER,
             topCcmtsEqamTemplateChnlPmtInterval            INTEGER,
             topCcmtsEqamTemplateChnlSdvSwitch            INTEGER,
             topCcmtsEqamTemplateChnlSdvPortStart            INTEGER
           }

topCcmtsEqamTemplateChnlChnlId OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The EQAM downstream channel ID."
    ::={ topCcmtsEqamTemplateChnlEntry 1 }

topCcmtsEqamTemplateChnlTsIDOffset OBJECT-TYPE
    SYNTAX        INTEGER (0..65535)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Transport stream ID offset,the final TSID of EQAM channel = TSID_base + TSID_offset, default value is current channel ID"
    ::={ topCcmtsEqamTemplateChnlEntry 2 }   
    
topCcmtsEqamTemplateChnlQamGroupName   OBJECT-TYPE
    SYNTAX        DisplayString (SIZE (1..63)) 
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "QAM group name, default value is qam_group_%d(%d is channel id)"
    ::={ topCcmtsEqamTemplateChnlEntry 3 }

topCcmtsEqamTemplateChnlQamManager OBJECT-TYPE
    SYNTAX      INTEGER{
					vod(1),    
					svod(2),
					broadcast(4)
				}
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "QAM manager configuration."
    ::={ topCcmtsEqamTemplateChnlEntry 4 }

topCcmtsEqamTemplateChnlOriginalNetworkId OBJECT-TYPE
    SYNTAX        INTEGER (0..65535)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Original network ID, default value is current channel ID"
    ::={ topCcmtsEqamTemplateChnlEntry 5 }

topCcmtsEqamTemplateChnlPatInterval OBJECT-TYPE
    SYNTAX        INTEGER (0 | 100..1000)   
    UNITS         "millisecond"
    MAX-ACCESS    read-write  
    STATUS        current
    DESCRIPTION
        "PAT packet sending period, value 0 means do not sending PAT."  
    DEFVAL {100}  
    	    ::={ topCcmtsEqamTemplateChnlEntry 6 }

topCcmtsEqamTemplateChnlPmtInterval OBJECT-TYPE
    SYNTAX   	  INTEGER (0 | 100..1000)  
    UNITS         "millisecond"
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "PMT packet sending period,value 0 means do not sending PMT."  
    DEFVAL {100} 
    ::={ topCcmtsEqamTemplateChnlEntry 7 }

topCcmtsEqamTemplateChnlSdvSwitch OBJECT-TYPE
    SYNTAX      INTEGER{
					enable(1),    
					disable(2)
				}
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "SDV switch."  
    DEFVAL {disable} 
    ::={ topCcmtsEqamTemplateChnlEntry 8 }

topCcmtsEqamTemplateChnlSdvPortStart OBJECT-TYPE
    SYNTAX        INTEGER (1..65535)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "SDV port start value." 
    DEFVAL {1} 
    ::={ topCcmtsEqamTemplateChnlEntry 9 }

topCcmtsEqamTemplateStreamMapTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TopCcmtsEqamTemplateStreamMapEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The EQAM template TS stream mappingping table."
    ::={ topCcmtsQamObject 4 }

topCcmtsEqamTemplateStreamMapEntry OBJECT-TYPE
    SYNTAX        TopCcmtsEqamTemplateStreamMapEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry of topCcmtsEqamTemplateStreamMapTable."
    INDEX  { topCcmtsEqamTemplateId,
           topCcmtsEqamTemplateChnlChnlId,
           topCcmtsEqamTemplateStreamMapId           }
    ::= { topCcmtsEqamTemplateStreamMapTable 1 }

TopCcmtsEqamTemplateStreamMapEntry ::= SEQUENCE {
             topCcmtsEqamTemplateStreamMapId            		INTEGER,
             topCcmtsEqamTemplateStreamMapIpType            	InetAddressType,
             topCcmtsEqamTemplateStreamMapDstIp            		InetAddress,
             topCcmtsEqamTemplateStreamMapUdpDstPort            INTEGER,
             topCcmtsEqamTemplateStreamMapSrcIp            		InetAddress,
             topCcmtsEqamTemplateStreamMapStreamType            INTEGER,
             topCcmtsEqamTemplateStreamMapInputProgramId    	INTEGER,
             topCcmtsEqamTemplateStreamMapOutputProgramId    	INTEGER,
             topCcmtsEqamTemplateStreamMapPmv            		INTEGER,
             topCcmtsEqamTemplateStreamMapAdminStatus             INTEGER,
             topCcmtsEqamTemplateStreamMapRowStatus            	RowStatus
           }

topCcmtsEqamTemplateStreamMapId OBJECT-TYPE
    SYNTAX        INTEGER(1..32)
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "EQAM TS stream mapping ID."
    ::={ topCcmtsEqamTemplateStreamMapEntry 1 }

topCcmtsEqamTemplateStreamMapIpType OBJECT-TYPE
    SYNTAX        InetAddressType
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "EQAM TS stream mapping source or destination IP address type."
    DEFVAL {ipv4} 
    ::={ topCcmtsEqamTemplateStreamMapEntry 2 }

topCcmtsEqamTemplateStreamMapDstIp OBJECT-TYPE
    SYNTAX        InetAddress
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS stream destination IP address,0 indicates TS destination IP is EQAM service IP as matching."   
    DEFVAL {0} 
    ::={ topCcmtsEqamTemplateStreamMapEntry 3 }

topCcmtsEqamTemplateStreamMapUdpDstPort OBJECT-TYPE
    SYNTAX        INTEGER (0..65535)
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS stream UDP destination port,default value is TS mapping ID." 
    ::={ topCcmtsEqamTemplateStreamMapEntry 4 }

topCcmtsEqamTemplateStreamMapSrcIp OBJECT-TYPE
    SYNTAX        InetAddress
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS stream source IP address,0 indicates TS sourse IP is ignored as matching."   
    DEFVAL {0} 
    ::={ topCcmtsEqamTemplateStreamMapEntry 5 }

topCcmtsEqamTemplateStreamMapStreamType OBJECT-TYPE
    SYNTAX        INTEGER{
					mpts(1),    
					data(2),
					spts(3),
					datar(4)
				}
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS stream type"   
    DEFVAL {data}
    ::={ topCcmtsEqamTemplateStreamMapEntry 6 }

topCcmtsEqamTemplateStreamMapInputProgramId OBJECT-TYPE
    SYNTAX        INTEGER (0..65535)
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "Input program number,0 indicates match the first program."   
    DEFVAL {0} 
    ::={ topCcmtsEqamTemplateStreamMapEntry 7 }

topCcmtsEqamTemplateStreamMapOutputProgramId OBJECT-TYPE
    SYNTAX        INTEGER (0..65535)
    MAX-ACCESS    read-write
    STATUS        current
    DESCRIPTION
        "Output program number,default value is TS mapping ID."  
    ::={ topCcmtsEqamTemplateStreamMapEntry 8 }

topCcmtsEqamTemplateStreamMapPmv OBJECT-TYPE
    SYNTAX        INTEGER (1..510)
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "PMV is related to video and audio PID of program stream,default value is TS mapping ID." 
    ::={ topCcmtsEqamTemplateStreamMapEntry 9 }

topCcmtsEqamTemplateStreamMapAdminStatus   OBJECT-TYPE
    SYNTAX        INTEGER
                  {
                     enable(1),
                     disable(2)
                  }
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "TS admin status."     
    DEFVAL{enable}
    ::={ topCcmtsEqamTemplateStreamMapEntry 10 }   
    
topCcmtsEqamTemplateStreamMapRowStatus OBJECT-TYPE
    SYNTAX        RowStatus
    MAX-ACCESS    read-create
    STATUS        current
    DESCRIPTION
        "Table row status."
    ::={ topCcmtsEqamTemplateStreamMapEntry 11 }

topCcmtsEqamStreamInfoTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TopCcmtsEqamStreamInfoEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream information table."
    ::={ topCcmtsQamObject 5 }

topCcmtsEqamStreamInfoEntry OBJECT-TYPE
    SYNTAX        TopCcmtsEqamStreamInfoEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry of topCcmtsEqamStreamInfoTable, index 'ifIndex' is CMTS ifIndex."
    INDEX  { ifIndex,
           topCcmtsEqamStreamInfoMappingId           }
    ::= { topCcmtsEqamStreamInfoTable 1 }

TopCcmtsEqamStreamInfoEntry ::= SEQUENCE {
             topCcmtsEqamStreamInfoMappingId            INTEGER,
             topCcmtsEqamStreamInfoIpType            InetAddressType,
             topCcmtsEqamStreamInfoSrcIp            InetAddress,
             topCcmtsEqamStreamInfoDstIp            InetAddress,
             topCcmtsEqamStreamInfoDstPort            INTEGER,
             topCcmtsEqamStreamInfoVideoType            INTEGER,
             topCcmtsEqamStreamInfoInputPmtPID            INTEGER,
             topCcmtsEqamStreamInfoOutputPmtPID            INTEGER,
             topCcmtsEqamStreamInfoInputRate            INTEGER,
             topCcmtsEqamStreamInfoOutputRate            INTEGER,
             topCcmtsEqamStreamInfoOutputProgramId            INTEGER,
             topCcmtsEqamStreamInfoStatus            BITS,
             topCcmtsEqamStreamInfoOperStatus            		INTEGER,    
             topCcmtsEqamStreamInfoSrcUdpPort            		INTEGER,       
             topCcmtsEqamStreamInfoInputProgramId                INTEGER,   
             topCcmtsEqamStreamInfoInputVideoPid            		INTEGER,    
             topCcmtsEqamStreamInfoInputAudioPid            		INTEGER,    
             topCcmtsEqamStreamInfoInputPcrPid            		INTEGER,      
             topCcmtsEqamStreamInfoOutputVideoPid            	INTEGER,   
             topCcmtsEqamStreamInfoOutputAudioPid            	INTEGER,   
             topCcmtsEqamStreamInfoOutputPcrPid            		INTEGER, 
             topCcmtsEqamStreamInfoInputEcmPid   		INTEGER, 
             topCcmtsEqamStreamInfoNumElems      		INTEGER, 
             topCcmtsEqamStreamInfoNumEcms       		INTEGER, 
             topCcmtsEqamStreamInfoPmtVersion    		INTEGER, 
             topCcmtsEqamStreamInfoOutputEcmPid   		INTEGER
           }

topCcmtsEqamStreamInfoMappingId OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "EQAM TS stream mapping ID."
    ::={ topCcmtsEqamStreamInfoEntry 1 }

topCcmtsEqamStreamInfoIpType OBJECT-TYPE
    SYNTAX        InetAddressType
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "EQAM TS stream mapping source or destination IP address type."
    ::={ topCcmtsEqamStreamInfoEntry 2 }

topCcmtsEqamStreamInfoSrcIp OBJECT-TYPE
    SYNTAX        InetAddress
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Source IP address."
    ::={ topCcmtsEqamStreamInfoEntry 3 }

topCcmtsEqamStreamInfoDstIp OBJECT-TYPE
    SYNTAX        InetAddress
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Destination IP address."
    ::={ topCcmtsEqamStreamInfoEntry 4 }

topCcmtsEqamStreamInfoDstPort OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Destination UDP port."
    ::={ topCcmtsEqamStreamInfoEntry 5 }

topCcmtsEqamStreamInfoVideoType OBJECT-TYPE
    SYNTAX        INTEGER{
	                      kUnkown(0),
	                      kMPEG1_VIDEO(1),
                          kMPEG2_VIDEO(2),
                          kMPEG1_AUDIO(3),
                          kMPEG2_AUDIO(4),
                          kAAC1_AUDIO(6),
                          kAAC_AUDIO(15),
                          kMPEG4_VIDEO(16),
                          kH264_VIDEO(27),
						  kH265_VIDEO(36),
                          kAVS_VIDEO(66),
                          kAAC2_AUDIO(129)
	                     }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Video format type."
    ::={ topCcmtsEqamStreamInfoEntry 6 }

topCcmtsEqamStreamInfoInputPmtPID OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Input program map table PID,default value 8191 indicates unknown information."
    ::={ topCcmtsEqamStreamInfoEntry 7 }

topCcmtsEqamStreamInfoOutputPmtPID OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Output program map table PID,default value 8191 indicates empty TS or unknown information."
    ::={ topCcmtsEqamStreamInfoEntry 8 }

topCcmtsEqamStreamInfoInputRate OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Input TS bits rate."
    ::={ topCcmtsEqamStreamInfoEntry 9 }

topCcmtsEqamStreamInfoOutputRate OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Output  TS bits rate."
    ::={ topCcmtsEqamStreamInfoEntry 10 }

topCcmtsEqamStreamInfoOutputProgramId OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "Output program ID."
    ::={ topCcmtsEqamStreamInfoEntry 11 }

topCcmtsEqamStreamInfoStatus OBJECT-TYPE
	SYNTAX		BITS {
	                ts_comming(0),
	                ts_analysing(1),
	                ts_analyse_success(2)
	                }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream  status."
    ::={ topCcmtsEqamStreamInfoEntry 12 }

 topCcmtsEqamStreamInfoOperStatus     OBJECT-TYPE
    SYNTAX        INTEGER
	              {
                    enable(1),
                    disable(2)
	              }
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream operation status."
	::={ topCcmtsEqamStreamInfoEntry 13 }
	
 topCcmtsEqamStreamInfoSrcUdpPort     OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream source UDP port."
	::={ topCcmtsEqamStreamInfoEntry 14 }
	
 topCcmtsEqamStreamInfoInputProgramId OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream input program ID."
	::={ topCcmtsEqamStreamInfoEntry 15 }
	
 topCcmtsEqamStreamInfoInputVideoPid  OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream input video PID,default value 8191 indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 16 }
	
 topCcmtsEqamStreamInfoInputAudioPid  OBJECT-TYPE  
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream input audio PID, high 16 bits is the first audio PID, low 16 bits is the second audio PID
		,default value '0x1FFF | (0x1FFF << 16)' indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 17 } 
	
 topCcmtsEqamStreamInfoInputPcrPid    OBJECT-TYPE 
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream input PCR PID,default value 8191 indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 18 }  
	
 topCcmtsEqamStreamInfoOutputVideoPid OBJECT-TYPE 
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream output video PID,default value 8191 indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 19 }
	
 topCcmtsEqamStreamInfoOutputAudioPid OBJECT-TYPE   
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream output audio PID, high 16 bits is the first audio PID, low 16 bits is the second audio PID
		,default value '0x1FFF | (0x1FFF << 16)' indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 20 } 
	
 topCcmtsEqamStreamInfoOutputPcrPid   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream output PCR PID,default value 8191 indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 21 } 
	
 topCcmtsEqamStreamInfoInputEcmPid   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream input ECM input PID
		,default value '0x1FFF | (0x1FFF << 16)' indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 22 } 

   
  topCcmtsEqamStreamInfoNumElems   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream element number."
	::={ topCcmtsEqamStreamInfoEntry 23} 
	
  topCcmtsEqamStreamInfoNumEcms   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream ECM number."
	::={ topCcmtsEqamStreamInfoEntry 24} 
	
 topCcmtsEqamStreamInfoPmtVersion   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream PMT version."
	::={ topCcmtsEqamStreamInfoEntry 25} 
	
 topCcmtsEqamStreamInfoOutputEcmPid   OBJECT-TYPE
    SYNTAX        INTEGER
    MAX-ACCESS    read-only
    STATUS        current
    DESCRIPTION
        "The EQAM TS stream output ECM input PID
		,default value '0x1FFF | (0x1FFF << 16)' indicates unknown information."
	::={ topCcmtsEqamStreamInfoEntry 26 } 
 
topCcmtsEqamTemplateBindTable OBJECT-TYPE
    SYNTAX        SEQUENCE OF TopCcmtsEqamTemplateBindEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "The EQAM template binding table."
    ::={ topCcmtsQamObject 6 }

topCcmtsEqamTemplateBindEntry OBJECT-TYPE
    SYNTAX        TopCcmtsEqamTemplateBindEntry
    MAX-ACCESS    not-accessible
    STATUS        current
    DESCRIPTION
        "An entry of topCcmtsEqamTemplateBindTable,index 'ifIndex' is CMTS ifIndex."
