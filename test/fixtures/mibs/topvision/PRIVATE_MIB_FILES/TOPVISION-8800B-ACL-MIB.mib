	SYNTAX 		INTEGER (0..65536) 
	MAX-ACCESS 	read-create
	STATUS 		current
	DESCRIPTION
		"Match L4 destination port,default value is 65536,not support 8820f"
	::= { topCcmtsAclCfgTableEntry 18 }
	
topActionMask OBJECT-TYPE
	SYNTAX 		BITS
				{
					deny(0),   
					permit(1),
					replaceCos(2),
					addVlan(3),
					removeVlan(4),
					copyToCpu(5),
					replaceTpid(6),
					replaceCfi(7),
					replaceVlanId(8),
					addVlanTpid(9),
					addVlanCfi(10),
					replaceiptos(11),
					replaceipdscp(12),
					addVlanCos(13),
					reserved6(14),
					reserved7(15)
				}
	MAX-ACCESS 	read-create
	STATUS 		current
	DESCRIPTION
		"
		BIT0: 1-deny enable; 0-deny disable; 
		BIT1: 1-permit enable; 0-permit disable;
		BIT2: 1-replace cos; 0-keep the current cos value
		BIT3: 1-add vlan; 0-keep the current vlan stacking
		BIT4: 1-remove vlan 0- keep the current vlan stacking
		BIT5: 1-copy data to cpu,0-default.
		BIT6: 1-replace tpid; 0-keep the current tpid value
		BIT7: 1-replace cfi; 0-keep the current cfi value,not support 8820f
		BIT8: 1-replace vlanid; 0-keep the current vlanid value
		BIT9: 1-add vlanTpid; 0-keep the current vlan stacking
 		BIT10: 1-add vlanCfi; 0-keep the current vlan stacking,not support 8820f
		BIT11: 1-replace ip tos; 0-keep the current cfi value(if replace ip dscp not set),not support 8820f
		BIT12: 1-replace ip dscp; 0-keep the current vlanid value(if replace ip tos not set)
		BIT13: 1-add vlanCos; 0-keep the current vlan stacking	
		other bits currently reserved.
		"
	::= { topCcmtsAclCfgTableEntry 19 }  

