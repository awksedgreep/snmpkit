topCcmtsChnlUtilThresholdTable OBJECT-TYPE
	SYNTAX		SEQUENCE OF TopCcmtsChnlUtilThresholdEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The threshold for the channel utilization to trigger alarm."
	::={ topCcmtsSystemObject 23 }

topCcmtsChnlUtilThresholdEntry OBJECT-TYPE
	SYNTAX		TopCcmtsChnlUtilThresholdEntry
	MAX-ACCESS	not-accessible
	STATUS		current
	DESCRIPTION
		"The threshold for the channel utilization to trigger alarm."
	INDEX { ifIndex }
	::={ topCcmtsChnlUtilThresholdTable 1 }

TopCcmtsChnlUtilThresholdEntry ::=SEQUENCE {
	topCcmtsUpChnlUtilMinorAlarmThreshold	Unsigned32,
	topCcmtsUpChnlUtilMinorRecoveryThreshold	Unsigned32,
	topCcmtsUpChnlUtilMajorAlarmThreshold	Unsigned32,
	topCcmtsUpChnlUtilMajorRecoveryThreshold	Unsigned32,
	topCcmtsUpChnlUtilCriticalAlarmThreshold	Unsigned32,
	topCcmtsUpChnlUtilCriticalRecoveryThreshold	Unsigned32,
	topCcmtsDownChnlUtilMinorAlarmThreshold	Unsigned32,
	topCcmtsDownChnlUtilMinorRecoveryThreshold	Unsigned32,
	topCcmtsDownChnlUtilMajorAlarmThreshold	Unsigned32,
	topCcmtsDownChnlUtilMajorRecoveryThreshold	Unsigned32,
	topCcmtsDownChnlUtilCriticalAlarmThreshold	Unsigned32,
	topCcmtsDownChnlUtilCriticalRcoeveryThreshold	Unsigned32
}

