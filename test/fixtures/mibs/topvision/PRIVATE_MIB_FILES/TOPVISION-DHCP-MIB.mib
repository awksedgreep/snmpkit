-----------------------------------------------------------------------
--topDhcpBundleTable
-----------------------------------------------------------------------	


topDhcpBundleTable OBJECT-TYPE
		SYNTAX      SEQUENCE OF TopDhcpBundleEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This Object describes bundle interface on CMTS."
        ::= { topDhcpObject 6 }

topDhcpBundleEntry OBJECT-TYPE
		SYNTAX      TopDhcpBundleEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This Object describes bundle interface on CMTS."
		INDEX { topDhcpBundleGroup,
				topDhcpBundleId }
        ::= { topDhcpBundleTable 1 }

TopDhcpBundleEntry ::= SEQUENCE {
		topDhcpBundleGroup                           INTEGER,
		topDhcpBundleId                              INTEGER,
		topDhcpBundlePolicy		                    INTEGER,
		topDhcpBundleCableSourceVerify				INTEGER,
		topDhcpBundleIpv6CableSourceVerify           INTEGER,
		topDhcpBundleDescription                     OCTET STRING,
		topDhcpBundleStatus		                    RowStatus
}



topDhcpBundleGroup OBJECT-TYPE
        SYNTAX      INTEGER(0..500)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "DHCP bundle group of device.
		If the device is remote MAC-PHY device controller, the range of group is 1 ~ 500.
		If the device is remote MAC-PHY device, there is no group, value is 0."
        ::= { topDhcpBundleEntry 1 }  


topDhcpBundleId OBJECT-TYPE
        SYNTAX      INTEGER(1..32)
        MAX-ACCESS  read-only
        STATUS      current
        DESCRIPTION
            "The ID of DHCP bundle interface."
        ::= { topDhcpBundleEntry 2 } 
		
topDhcpBundlePolicy OBJECT-TYPE
        SYNTAX      INTEGER{
						primary(1),
						policy(2),
						strict(3)
					}
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "The type of the device under this bundle the Giaddr used for.
	    Primary means all of the devices use the primary IP address for Giaddr. Policy means the CM use the primary 
	    IP address and other device use the first secondary IP address as it's Giaddr. If set strict(3), 
		need to set topDhcpGiAddrTable to complet Giaddr."
		DEFVAL          { 1 }
        ::= { topDhcpBundleEntry 3 }  
 
topDhcpBundleCableSourceVerify	 OBJECT-TYPE
        SYNTAX      INTEGER{
						on(1),
						off(2),
						dhcp(3)
					}
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "DHCP Relay Cable source Verify.
            on(1): means the device does verify ip and mac for packets,then drop any packets verified failed.
            off(2): means the device does not verify ip and mac for packets.
            dhcp(3): means the device does verify ip and mac for packets, but will re-verify the cpe whose packets be verified failed."
		DEFVAL     { 1 }
        ::= { topDhcpBundleEntry 4 }  

		
topDhcpBundleIpv6CableSourceVerify	 OBJECT-TYPE
        SYNTAX      INTEGER{
						on(1),
						off(2),
						dhcp(3)
					}
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "DHCP Relay IPv6 Cable source Verify.
            on(1): means the device does verify IPv6 and mac for packets,then drop any packets verified failed.
            off(2): means the device does not verify IPv6 and mac for packets.
            dhcp(3): means the device does verify IPv6 and mac for packets, but will re-verify the cpe whose packets be verified failed"
		DEFVAL     { 1 }
        ::= { topDhcpBundleEntry 5 }  
		
	
topDhcpBundleDescription OBJECT-TYPE
        SYNTAX      OCTET STRING(SIZE(0..255))
        MAX-ACCESS  read-create
        STATUS      current
        DESCRIPTION
            "Description of the DHCP bundle."
        ::= { topDhcpBundleEntry 6 }  

		
topDhcpBundleStatus  OBJECT-TYPE
	SYNTAX          RowStatus
	MAX-ACCESS      read-create
	STATUS          current
	DESCRIPTION
		"Controls and reflects the status of rows in this
		table.  Rows in this table may be created by 
		create-and-go  paradigm.  There is no
		restriction on changing values in a row of this table
		while the row is active." 
	::= { topDhcpBundleEntry 7 }



	
