topDhcpConfigObject OBJECT IDENTIFIER ::= { topDhcpObject 1}





-----------------------------------------------------------------------
--topDhcpModeTable
-----------------------------------------------------------------------	
topDhcpModeTable OBJECT-TYPE
		SYNTAX      SEQUENCE OF TopDhcpModeEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table describes the DHCP relay mode for device."
        ::= { topDhcpObject 2 }

topDhcpModeEntry OBJECT-TYPE
		SYNTAX      TopDhcpModeEntry
        MAX-ACCESS  not-accessible
        STATUS      current
        DESCRIPTION
            "This table describes the DHCP relay mode for device."
		INDEX { ifIndex ,
				topDhcpDevType }
        ::= { topDhcpModeTable 1 }

TopDhcpModeEntry ::= SEQUENCE {
	topDhcpDevType        INTEGER,
	topDhcpMode            INTEGER,
	topDhcpv6Mode          INTEGER
}


topDhcpDevType OBJECT-TYPE
		SYNTAX	INTEGER {		    
						cm(1),
						host(2),
						mta(3),
						stb(4)
					}
	MAX-ACCESS read-only
	STATUS current
	DESCRIPTION
		"Device type."
    ::= { topDhcpModeEntry 1 } 
	

topDhcpMode OBJECT-TYPE
		SYNTAX	INTEGER {
			snooping (1),
			l2-relay (2),
			l3-relay (3)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
	"DHCP mode: 1: snooping; 2: layer 2 relay; 3: layer 3 relay."
	DEFVAL { snooping }
    ::= { topDhcpModeEntry 2 } 

topDhcpv6Mode OBJECT-TYPE
		SYNTAX	INTEGER {
			snooping (1),
			l2-relay (2),
			l3-relay (3),
			ldra (4)
		}
	MAX-ACCESS read-write
	STATUS current
	DESCRIPTION
	"DHCPv6 mode: 1: snooping; 2: layer 2 relay; 3: layer 3 relay; 4: Lightweight DHCPv6 Relay Agent."
	DEFVAL { snooping }
    ::= { topDhcpModeEntry 3 } 
	
	


